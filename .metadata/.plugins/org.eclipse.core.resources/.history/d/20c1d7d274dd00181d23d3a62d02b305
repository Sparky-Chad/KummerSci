package pong;

import java.awt.*;

//object to help implement for other classes
public class Sprite {
	private int x;
	//displace x coords
	private int y;
	//y coords
	private int width;
	//width of the object
	private int height;
	//height of the object
	private Image image;
	//image object
	
	public void setX(int x) {
		this.x = x;
	}
	public int getX() {
		return x;
	}
	public void setY(int y) {
		this.y = y;
	}
	public int getY() {
		return y;
	}
	
}
